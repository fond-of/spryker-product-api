{
  "name": "fond-of-spryker/product-api",
  "description": "Extends the Base Spryker Product Api Module",
  "license": "MIT",
  "authors": [
    {
      "name": "FOND OF",
      "email": "spryker@fondof.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "spryker/product-api": "^0.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.12",
    "codeception/codeception": "^2.5",
    "codeception/mockery-module": "^0.2",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1",
    "fond-of-codeception/spryker": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "tests/Generated/"
    }
  }
}
